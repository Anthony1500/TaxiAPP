<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/nav_maps">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.taxiapp.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.taxiapp.ui.gallery.GalleryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.taxiapp.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" />
    <fragment
        android:id="@+id/nav_informacion"
        android:name="com.example.taxiapp.ui.informacion.informacionfragment"
        android:label="@string/menu_informacion"
        tools:layout="@layout/fragment_informacion" />
    <fragment
        android:id="@+id/nav_ayuda"
        android:name="com.example.taxiapp.ui.ayuda.ayudafragment"
        android:label="Ayuda"
        tools:layout="@layout/fragment_ayuda" />
    <fragment
        android:id="@+id/nav_subirfoto"
        android:name="com.example.taxiapp.subir_foto"
        android:label="Perfil Usuario"
        tools:layout="@layout/subir_foto" />
    <fragment
        android:id="@+id/nav_maps"
        android:name="com.example.taxiapp.ui.maps.mapsFragment"
        android:label="Mapa"
        tools:layout="@layout/fragment_maps" />
    <fragment
        android:id="@+id/nav_datosusu"
        android:name="com.example.taxiapp.ui.gallery.datos"
        android:label="MIS DATOS"
        tools:layout="@layout/datosusua" />
    <action
        android:id="@+id/datos_llamado"
        app:destination="@+id/nav_datosusu" />
    <fragment
        android:id="@+id/tema"
        android:name="com.example.taxiapp.ui.gallery.tema"
        android:label="TEMA"
        tools:layout="@layout/tema" />

    <action
        android:id="@+id/tema_llamado"
        app:destination="@+id/tema" />



</navigation>